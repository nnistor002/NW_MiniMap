{
  "name": "nw_minimap",
  "version": "0.1.0",
  "homepage": "./",
  "author": "Natanael Nistor",
  "description": "A personal resource tracker mini map for New World.",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "aser": "^1.0.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "eslint": "^7.32.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-map-interaction": "^2.1.0",
    "react-redux": "^7.2.5",
    "react-responsive-pinch-zoom-pan": "^0.3.0",
    "react-scripts": "4.0.3",
    "react-zoom-pan-pinch": "^2.1.3",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.0.1"
  },
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-react": "concurrently \" cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "yarn build && electron-builder",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron:step1": "yarn build",
    "electron:step2": "electron-builder"
  },
  "build": {
    "extends": null,
    "appId": "com.example.NW_MiniMap",
    "productName": "NW_MiniMap",
    "asar": false,
    "files": [
      "build/**/*",
      "main.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^14.0.0",
    "electron-builder": "^22.10.5",
    "electron-packager": "^15.4.0"
  }
}
